{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ".*spec.*"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "logoFile": "./Shingo_Logo.png",
    "cleverLinks": true,
    "monospaceLinks": true,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "Shingo Affiliate Portal",
    "footer": "",
    "copyright": "Shingo Affiliates Portal Copyright Â© 2017 Shingo Institute.",
    "navType": "vertical",
    "theme": "lumen",
    // "syntaxTheme": "dark",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "protocol": "html://",
    "methodHeadingReturns": false
  },
  "markdown": {
    "parser": "gfm"
  }
}
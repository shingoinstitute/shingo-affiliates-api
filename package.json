{
  "name": "shingo-affiliates-api",
  "version": "2.0.0",
  "description": "Provide REST services to interact with Shingo Affiliates and Workshops.",
  "main": "index.js",
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@nestjs/cqrs": "^5.1.1",
    "@types/connect-redis": "0.0.6",
    "@types/express": "^4.0.35",
    "@types/nodemailer": "^3.1.2",
    "@types/winston": "^2.3.4",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.3.2",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "es6-symbol": "^3.1.1",
    "express-session": "^1.15.6",
    "fp-ts": "^1.14.4",
    "generate-password": "^1.4.1",
    "grpc": "^1.19.0",
    "grpc-promise": "^1.1.0",
    "immer": "^2.1.5",
    "jsforce": "^1.9.1",
    "jwt-simple": "^0.5.1",
    "multer": "^1.3.0",
    "node-cache": "^4.1.1",
    "nodemailer": "^4.0.1",
    "object-hash": "^1.1.8",
    "qs": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rxjs": "^6.4.0",
    "ts-mockito": "^2.0.2",
    "winston": "^2.3.1"
  },
  "scripts": {
    "start": "nodemon -e ts --exec \"npm run compile\"",
    "compile": "npm run build && node ./build/index.js",
    "build": "npm run build:ts && npm run build:sf",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "build:sf": "ts-node ./generate-sf-interfaces.ts",
    "test": "tsc -p ./tsconfig.spec.json; cd spec; alsatian ../**/*.spec.js",
    "coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report",
    "clean": "rm -r build/ && rm -r coverage && rm -r .nyc_output && rm -r spec"
  },
  "author": "Dustin E. Homan",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/testing": "^6.0.5",
    "@shingo/describe2ts": "^0.1.0",
    "@shingo/tslint-rules": "^1.1.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/debug": "^4.1.2",
    "@types/jest": "^24.0.11",
    "@types/jsforce": "^1.9.5",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.3",
    "@types/qs": "^6.5.2",
    "alsatian": "^2.0.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*mock*"
    ]
  }
}

{
  "name": "shingo-affiliates-api",
  "version": "2.0.0",
  "description": "Provide REST services to interact with Shingo Affiliates and Workshops.",
  "main": "./build/index.js",
  "dependencies": {
    "@nestjs/common": "^2.0.3",
    "@nestjs/core": "^2.0.5",
    "@nestjs/cqrs": "^2.0.1",
    "@nestjs/microservices": "^2.0.5",
    "@nestjs/testing": "^2.0.3",
    "@nestjs/websockets": "^2.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.3.2",
    "cors": "^2.8.4",
    "es6-symbol": "^3.1.1",
    "express-session": "^1.15.6",
    "generate-password": "^1.3.0",
    "grpc": "^1.12.4",
    "grpc-promise": "^1.1.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.3",
    "jsforce": "^1.9.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "multer": "^1.3.0",
    "node-cache": "^4.1.1",
    "nodemailer": "^4.0.1",
    "object-hash": "^1.1.8",
    "reflect-metadata": "^0.1.10",
    "request": "^2.87.0",
    "rxjs": "^5.4.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/body-parser": "^1.17.0",
    "@types/connect-redis": "0.0.6",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.0.35",
    "@types/lodash": "^4.14.115",
    "@types/node": "^7.0.39",
    "@types/node-cache": "^4.1.1",
    "@types/nodemailer": "^3.1.2",
    "@types/object-hash": "^1.2.0",
    "@types/winston": "^2.3.4",
    "alsatian": "^2.0.0",
    "nodemon": "^1.18.3",
    "ts-mockito": "^2.0.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "debug": "nodemon --watch ./build --inspect=0.0.0.0:9222 --nolazy ./build/index.js",
    "docker-debug": "docker-compose -f docker-compose.debug.yml up",
    "build": "tsc -p ./tsconfig.json && cp -R ./proto build/proto",
    "test": "tsc -p ./tsconfig.spec.json; cd spec; alsatian ./**/*.spec.js",
    "coverage": "nyc --reporter=lcov --reporter=html npm run test && nyc report",
    "docs": "tsc && jsdoc -t ./node_modules/ink-docstrap/template/ -r build/ -d docs -c jsdoc.conf.json --readme ./README-DOCS.md && cp Shingo_Logo.png ./docs",
    "clean": "rm -r build/ && rm -r coverage && rm -r .nyc_output && rm -r spec"
  },
  "author": "Dustin E. Homan",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*mock*"
    ]
  }
}
